    "apollo-server-express": "^2.0.3",
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fb": "^2.0.0",
    "google-auth-library": "^1.6.1",
    "googleapis": "^33.0.0",
    "graphql": "^0.13.2",
